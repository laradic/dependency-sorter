{
    "name": "laradic/dependency-sorter",
    "description": "A dependency sorter package",
    "keywords": ["laradic", "dependencies", "laravel"],
    "license": "proprietary",
    "authors": [
        {"name": "Laradic", "homepage": "https://laradic.nl", "email": "info@laradic.nl"},
        {"name": "Robin Radic", "homepage": "http://robin.radic.nl", "email": "robin@laradic.nl", "role": "Developer"}
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "laradic/testing": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Laradic\\DependencySorter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laradic\\Tests\\DependencySorter\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
